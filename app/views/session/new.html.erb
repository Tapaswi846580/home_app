<% provide(:title,"Login")%>

<h1 align="center">Login</h1>

<div class="panel panel-default" style="margin-right:50px; margin-left:600px; width:400px">
<div class="panel-body" style="margin-right:50px; margin-left:50px;">
<div>
<%=form_for(:session,url: login_path) do |f|%>




	
	
	<%= f.label :email%>
	<br/>
	<%= f.email_field :email%>
	<br/>
	<br/>
    

	
	<%= f.label :password %>
	<br/>
<%= f.password_field :password %>
<br/>
<br/>

<%= f.submit "LogIn", class: "btn btn-primary" %>

<% end %>


</div>

</div>
</div>
</aside>
</div>